<?xml version="1.0"?>
<Container version="2">
  <Name>CloudCommander-HR</Name>
  <Repository>solipsist01/cloudcommander</Repository>   
  <Registry>https://hub.docker.com/r/coderaiser/cloudcmd/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support></Support>
  <Project/>
  <Overview>
CloudCommander is a simple web file browser with a built in text editor and archive compress/extract abilities.[br/]

[b]Instructions:[/b][br/]
Set Port to the port you want to be able to access from. Defaults to 8765[br/]
Set Mount Point to the directory you want to be able to access in the browser. Defaults to /mnt/user, all shares[br/]
	</Overview>
  <Category>Tools:</Category>
  <WebUI>http://[IP]:[PORT:8000]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/cheesemarathon/docker-templates/master/cheesemarathon/cloudcmd.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/cheesemarathon/docker-templates/master/images/cloudcmdLogo.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1641245969</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>&#xD;
[b]CloudCommander[/b] is a simple web file browser with a built in text editor and archive compress/extract abilities.[br/]&#xD;
&#xD;
]Instructions:&#xD;
Set Port to the port you want to be able to access from. Defaults to 8765[br/]&#xD;
Set Mount Point to the directory you want to be able to access in the browser. Defaults to /mnt/user, all shares[br/]&#xD;
	</Description>
  <Networking>
    <Mode>br0</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/</HostDir>
      <ContainerDir>/UNRAID</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/cloudcmd</HostDir>
      <ContainerDir>/root</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
    
  <Environment/>
    <Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>

  <Labels/>

    <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="Container Variable: PUID" Type="Variable" Display="always" Required="false" Mask="false">99</Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="Container Variable: PGID" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
  

  <Config Name="Host Path 2" Target="/usr/src/app" Default="" Mode="rw" Description="Container Path: /usr/src/app" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/cloudcmd/src/</Config>

  <Config Name="Mount Path" Target="/UNRAID" Default="" Mode="rw" Description="Container Path: /UNRAID" Type="Path" Display="always" Required="true" Mask="false">/</Config>
	<Config Name="Port" Target="8000" Default="8765" Mode="tcp" Description="Container Port: 8000" Type="Port" Display="always" Required="true" Mask="false">80</Config>
  
  <Config Name="HOME - Dont Change!" Target="HOME" Default="" Mode="" Description="Container Variable: HOME" Type="Variable" Display="advanced" Required="true" Mask="false">/root</Config>
</Container>
